  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --open --port 4200",
    "build": "webpack --config webpack.config.js --mode production",
    "test": "jest",
    "test:watch": "npm test --watch"
  },
  "jest": {
    "setupFiles": [
      "./jest/enzyme.js"
